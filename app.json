{
  "expo": {
    "name": "Shot Selector",
    "description": "Shot Selector was created to help you randomly select a shot out of your beverage collection.",
    "slug": "shot-selector",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": ["ios", "android"],
    "version": "5.1.0",
    "orientation": "portrait",
    "backgroundColor": "#f1c232",
    "primaryColor": "#f1c232",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1c232"
    },
    "updates": {
      "fallbackToCacheTimeout": 500
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.rtrdevelopment.shot-selector",
      "buildNumber": "5.1.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.rtrdevelopment.shot_selector",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f1c232"
      },
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "VIBRATE"]
    }
  }
}
